package Coralon;

import java.util.ArrayList;

public class Multimedia {

	private ArrayList<Copia>copia;
	
	public Multimedia() {
		copia=new ArrayList<Copia>();
	}
	public void agregarCopia(Copia c) {
		copia.add(c);
	}
	public void listarCopia() {
		for(Copia c:copia)
			System.out.println(c.impirmirCopia());
	}
	
}

