package Coralon;

public class Cliente {
	private String Nombre;
	private String Apellido;
	private int IdCliente;
		
	public String imprimirNombreApellido() {
		return "Cliente [Nombre=" + Nombre + ", Apellido=" + Apellido + "]";
	}
	public String imprimirCliente() {
		return "Cliente [Nombre=" + Nombre + ", Apellido=" + Apellido + ", IdCliente=" + IdCliente + "]";
	}
	
	public Cliente(String nombre, String apellido, int idCliente) {
		super();
		setNombre(nombre);
		setApellido(apellido);
		setIdCliente(idCliente);
	}
	public String getNombre() {
		return Nombre;
	}
	public void setNombre(String nombre) {
		Nombre = nombre;
	}
	public String getApellido() {
		return Apellido;
	}
	public void setApellido(String apellido) {
		Apellido = apellido;
	}
	public int getIdCliente() {
		return IdCliente;
	}
	public void setIdCliente(int idCliente) {
		IdCliente = idCliente;
	}
	
	
}
